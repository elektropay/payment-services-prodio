{
  "name": "ezpayPaymentTransactions",
  "plural": "ezpayPaymentTransactions",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "transactionId": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "payerId": {
      "type": "string"
    },
    "merchantId": {
      "type": "string"
    },
    "totalAmount":{
      "type":"string"
    },
    "metaData":{
      "type":"object"
    },
    "transactionStatus": {
      "type": "string"
    },
    "isActive":{
      "type":"boolean",
      "default":true
    },
    "createdAt": {
      "type": "date"
    },
    "paymentDate":{
      "type":"date"
    }
  },
  "validations": [],
  "relations": {
    "Payer": {
      "type": "belongsTo",
      "model": "ezpayPayees",
      "foreignKey": "payerId"
    },
    "Merchant": {
      "type": "belongsTo",
      "model": "ezpayMerchants",
      "foreignKey": "merchantId"
    }
  },
  "acls": [],
  "methods": {}
}
