{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "savedCardsMetaData": {
    "dataSource": "db",
    "public": true
  },
  "ezpayMerchants": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "testMerchant":true,
          "createMerchant": true,
          "getPayeesListing": true,
          "removeById": true,
          "removeMerchant": true,
          "deactivateMerchant": true,
          "getMerchantProfile": true,
          "getMerchantActivationStatus": true
        }
      }
    }
  },
  "ezpayPayees": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": true
        }
      }
    }
  },
  "ezpayPaymentTransactions": {
    "dataSource": "db",
    "public": true
  },
  "merchantPayeesRelation": {
    "dataSource": "db",
    "public": true
  }
}
