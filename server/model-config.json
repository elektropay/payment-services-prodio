{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "savedCardsMetaData": {
    "dataSource": "db",
    "public": true
  },
  "ezpayMerchants": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "createMerchant": true,
          "getPayeesListing": true,
          "removeById": true,
          "RemoveMerchant":true,
          "DeactivateMerchant":true,
          "GetMerchantProfile":true,
          "GetMerchantActivationStatus":true
        }
      }
    }
  },
  "ezpayPayees": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "addPayee": true,
          "editPayee": true,
          "removePayees": true,
          "importPayees":true,
          "addCardForPayee":true,
          "removeCard":true
        }
      }
    }
  },
  "ezpayPaymentTransactions": {
    "dataSource": "db",
    "public": true
  }
}
